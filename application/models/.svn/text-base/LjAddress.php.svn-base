<?php

class Application_Model_LjAddress extends Application_Model_LjBaseModel {

    //Modlular level variables
    protected $table_name = "address";
    protected $table_pk = "address_id";
    protected $class_name = 'Application_Model_LjAddress';
    var $address_id;
    var $posting_id;   
    var $address;   
    var $city;   
    var $zip;   
    var $lat;
    var $lon;
    var $date_created;

    //////////////////////////
    //Sets Entity info
    function setAddress_id($address_id) {
        $this->address_id = $address_id;
    }

    function setPosting_id($posting_id) {
        $this->posting_id = $posting_id;
    } 

    function setAddress($address) {
        $this->address = $address;
    }
  

    function setCity($city) {
        $this->city = $city;
    }

  
    function setZip($zip) {
        $this->zip = $zip;
    }


    function setLat($lat) {
        $this->lat = $lat;
    }

    function setLon($lon) {
        $this->lon = $lon;
    }

    function setDate_created($date_created) {
        $this->date_created = $date_created;
    }

    //////////////////////////
    //////////////////////////
    //Returns Loaded Entity info
    function getAddress_id() {
        return $this->address_id;
    }

    function getPosting_id() {
        return $this->posting_id;
    }
  

    function getAddress() {
        return $this->address;
    }
   
    function getCity() {
        return $this->city;
    }
  

    function getZip() {
        return $this->zip;
    }

   
    function getLat() {
        return $this->lat;
    }

    function getLon() {
        return $this->lon;
    }

    function getDate_created() {
        return $this->date_created;
    }

}

//End class

