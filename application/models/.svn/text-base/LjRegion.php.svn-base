<?php

class Application_Model_LjRegion extends Application_Model_LjBaseModel {

    //Modlular level variables
    protected $table_name = "region";
    protected $table_pk = "region_id";
    protected $class_name = 'Application_Model_LjRegion';
    var $region_id;
    var $metro_id;
    var $region;
    var $date_created;

    //////////////////////////
    //Sets Entity info
    function setRegion_id($region_id) {
        $this->region_id = $region_id;
    }

    function setMetro_id($metro_id) {
        $this->metro_id = $metro_id;
    }

    function setRegion($region) {
        $this->region = $region;
    }

    function setDate_created($date_created) {
        $this->date_created = $date_created;
    }

    //////////////////////////
    //////////////////////////
    //Returns Loaded Entity info
    function getRegion_id() {
        return $this->region_id;
    }

    function getMetro_id() {
        return $this->metro_id;
    }

    function getRegion() {
        return $this->region;
    }

    function getDate_created() {
        return $this->date_created;
    }

}

