    }

    function setUser_id($user_id) {
        $this->user_id = $user_id;
    }

    function setPosting_user_id($posting_user_id) {
        $this->posting_user_id = $posting_user_id;
    }

    function setRating($rating) {
        $this->rating = $rating;
    }

