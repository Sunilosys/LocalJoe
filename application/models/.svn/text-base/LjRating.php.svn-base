        $this->date_created = $date_created;
    }

    //////////////////////////
    //////////////////////////
    //Returns Loaded Entity info
    function getRating_id() {
        return $this->rating_id;
    }

    function getRating_scale_id() {
        return $this->rating_scale_id;
    }

    function getPosting_id() {
        return $this->posting_id;
    }

    function getUser_id() {
        return $this->user_id;
    }

    function getPosting_user_id() {
        return $this->posting_user_id;
    }

    function getRating() {
        return $this->rating;
    }

    function getDate_created() {
        return $this->date_created;
    }

}

