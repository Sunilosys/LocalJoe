    var $rating_id;
    var $rating_scale_id;
    var $posting_id;
    var $user_id;
    var $posting_user_id;
    var $rating;
    var $date_created;

    //////////////////////////
    //Sets Entity info
    function setRating_id($rating_id) {
        $this->rating_id = $rating_id;
    }

    function setRating_scale_id($rating_scale_id) {
