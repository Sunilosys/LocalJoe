    protected $table_pk = "image_type_id";
    protected $class_name = 'Application_Model_LjImageType';
    var $image_type_id;
    var $image_type;
    var $width;
    var $height;
    var $date_created;

    //////////////////////////
    //Sets Entity info
    function setImage_type_id($image_type_id) {
        $this->image_type_id = $image_type_id;
    }

