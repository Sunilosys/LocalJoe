/**
 * Description of LjCategory
 *
 * @author sunil_salunkhe
 */
class Application_Model_LjCategory extends Application_Model_LjBaseModel {

    protected $table_name = "category";
    protected $table_pk = "category_id";
    protected $class_name = 'Application_Model_LjCategory';
    protected $category_id;
    protected $parent_category_id;
    protected $name;
    protected $date_created;

    /**
     * @return the $category_id
     */
    public function getCategory_id() {
        return $this->category_id;
    }

    /**
     * @return the parent_category_id 
     */
    public function getParent_category_id() {
        return $this->parent_category_id;
    }

    /**
     * @return the name 
     */
    public function getCategoryName() {
        return $this->name;
    }

    /**
     * @return the $date_created
     */
    public function getDate_created() {
        return $this->date_created;
    }
    
     /**
     * set the $category_id
     */
    public function setCategory_id($category_id) {
        $this->category_id = $category_id;
    }

    /**
     * set the parent_category_id 
     */
    public function setParent_category_id($parent_category_id) {
        $this->parent_category_id = $parent_category_id;
    }

    /**
     * set the name 
     */
    public function setCategoryName($name) {
        $this->name = $name;
    }

    /**
     * set the $date_created
     */
    public function setDate_created($date_created) {
        $this->date_created = $date_created;
    }

}

