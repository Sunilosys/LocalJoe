<?php

class Application_Model_LjState extends Application_Model_LjBaseModel {

    //Modlular level variables
    protected $table_name = "state";
    protected $table_pk = "state_id";
    protected $class_name = 'Application_Model_LjState';
    var $state_id;
    var $country_id;
    var $state;
    var $date_created;

    //////////////////////////
    //Sets Entity info
    function setState_id($state_id) {
        $this->state_id = $state_id;
    }

    function setCountry_id($country_id) {
        $this->country_id = $country_id;
    }

    function setState($state) {
        $this->state = $state;
    }

    function setDate_created($date_created) {
        $this->date_created = $date_created;
    }

    //////////////////////////
    //////////////////////////
    //Returns Loaded Entity info
    function getState_id() {
        return $this->state_id;
    }

    function getCountry_id() {
        return $this->country_id;
    }

    function getState() {
        return $this->state;
    }

    function getDate_created() {
        return $this->date_created;
    }

}

//End class

