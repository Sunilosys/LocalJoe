<?php

class Application_Model_LjImage extends Application_Model_LjBaseModel {

    //Modlular level variables
    protected $table_name = "image";
    protected $table_pk = "image_id";
    protected $class_name = 'Application_Model_LjImage';
    var $image_id;
    var $posting_id;
    var $image_file;
    var $image_title;
    var $width;
    var $height;
    var $image_size;
    var $date_created;

    //////////////////////////
    //Sets Entity info
    function setImage_id($image_id) {
        $this->image_id = $image_id;
    }

    function setPosting_id($posting_id) {
        $this->posting_id = $posting_id;
    }

    function setImage_file($image_file) {
        $this->image_file = $image_file;
    }

    function setImage_title($image_title) {
        $this->image_title = $image_title;
    }

    function setWidth($width) {
        $this->width = $width;
    }

    function setHeight($height) {
        $this->height = $height;
    }

    function setImage_size($image_size) {
        $this->image_size = $image_size;
    }

    function setDate_created($date_created) {
        $this->date_created = $date_created;
    }

    //////////////////////////
    //////////////////////////
    //Returns Loaded Entity info
    function getImage_id() {
        return $this->image_id;
    }

    function getPosting_id() {
        return $this->posting_id;
    }

    function getImage_file() {
        return $this->image_file;
    }

    function getImage_title() {
        return $this->image_title;
    }

    function getWidth() {
        return $this->width;
    }

    function getHeight() {
        return $this->height;
    }

    function getImage_size() {
        return $this->image_size;
    }

    function getDate_created() {
        return $this->date_created;
    }

}

