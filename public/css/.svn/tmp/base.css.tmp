/* Buttons */

.button{
padding: 1px 3px; 
display: inline-block; 
margin: 5px 0;
padding: 7px; 
font-weight: normal !important;
text-decoration: none;
-moz-border-radius: 3px; 
-webkit-border-radius: 3px; 
border-radius: 3px;
text-align: center;
overflow: hidden;
cursor: pointer;
}
.button:hover{text-decoration: none;}

.button.large{
line-height: 20px;
letter-spacing: -.03em;
}
.button.medium-large{
padding: 3px 7px; 
height: auto;
}
.button.medium{
padding: 1px 3px; 
height: auto;
line-height: 18px;
}
.button.small{
font-size: 13px !important; 
padding: 3px 7px; 
line-height: 11px !important; 
font-weight: normal;
}

.button.primary{
background: #5E9BAE;
-moz-box-shadow: 1px 1px 0 #39616D;
-webkitbox-shadow: 1px 1px 0 #39616D;
box-shadow: 1px 1px 0 #39616D;
color: #fff;
text-shadow: -1px -1px 0 #39616D;
border: 0; 
cursor: pointer;
margin-right: 1px !important;
}
.button.secondary{
-moz-box-shadow: 1px 1px 0 #bbb, -1px -1px 0 #fff;
-webkitbox-shadow: 1px 1px 0 #bbb, -1px -1px 0 #fff;
box-shadow: 1px 1px 0 #bbb,  -1px -1px 0 #fff;
background: #ededed;
border: 0px solid #ccc;
color: #666;
text-shadow: none;
margin-right: 1px !important;
}

#ad-gallery .button{
background: #3f4545;
color: #fff;
text-shadow: 0;
text-transform: uppercase;
}

.searchwrap .button, .button.finish{
background: #98c22a !important;
color: #fff !important;
border: 0px solid #117F02 !important;
-moz-box-shadow: 1px 1px 0 #5d761c;
-webkit-box-shadow:  1px 1px 0 #5d761c;
box-shadow:  1px 1px 0 #5d761c;
text-shadow: -1px -1px #7f9d31;
}

.secondary:hover{border: 0px solid #aaa;}
.secondary:active{
background: #ddd; 
-moz-box-shadow: inset 1px 1px 2px #bbb;
-webkit-box-shadow: inset 1 1px 2px #bbb;
box-shadow: inset 1px 1px 2px #bbb;
}
.primary:active{
-moz-box-shadow: inset 1px 1px 2px #333;
-webkit-box-shadow: inset 1 1px 2px #333;
box-shadow: inset 1px 1px 2px #333;
}

.button.google, .button.facebook{
padding: 4px 7px 4px 24px; 
position: relative;
height: 16px; 
line-height: 16px;
background: #aaa;
color: #333;
border: 0 !important;
} 
.button.google .icon, .button.facebook .icon{
display: block; 
position: absolute;
top: 4px; 
left: 4px; 
height: 16px; 
width: 16px; 
background: url(../images/google-icon.png);
}
.button.facebook .icon{background: url(../images/facebook-icon.png);}

#searchform a.button {
-moz-border-radius: 0 4px 4px 0;
-webkit-border-radius: 0 4px 4px 0;
border-radius: 0 4px 4px 0;
float: left !important;
margin-left: -1px !important;
width: 79px;
font-size: 16px; 
font-weight: bold !important;
}
#searchform a.button:active{
-moz-box-shadow: inset 1px 1px 2px #333, 1px 1px 0 #5d761c;
-webkit-box-shadow: inset 1 1px 2px #333, 1px 1px 0 #5d761c;
box-shadow: inset 1px 1px 2px #333, 1px 1px 0 #5d761c;
}
