/* FORMS*/

input {border: 1px solid #999; height: 20px; line-height: 20px !important;}
input[type='radio'],
input[type='radio']:focus{
border: 0 !important; 
height: auto; 
line-height: inherit !important;
-moz-box-shadow: 0 0 0;
-webkit-box-shadow: 0 0 0;
box-shadow: 0 0 0 !important;
}
input {font-size: 12px !important; line-height: 13px; color: #333;font-style: normal;}
input:focus, .combobox.focus, .iPhoneCheckContainer.focus{
border: 1px solid #5E9BAE !important; 
-moz-box-shadow: 0 0 5px #5E9BAE, 0 0 10px #ddd inset !important;
-webkit-box-shadow: 0 0 5px #5E9BAE, 0 0 10px #ddd inset !important;
box-shadow: 0 0 5px #5E9BAE, 0 0 10px #ddd inset !important;
}
a.check{position: relative;}
a.check.focus:before{
display: block;
position: absolute; 
width: 10px; 
height: 10px;
content: '';
border: 1px solid #5E9BAE !important; 
-moz-box-shadow: 0 0 5px #5E9BAE, 0 0 10px #ddd inset !important;
-webkit-box-shadow: 0 0 5px #5E9BAE, 0 0 10px #ddd inset !important;
box-shadow: 0 0 5px #5E9BAE, 0 0 10px #ddd inset !important;
left: 0;
-moz-border-radius: 2px;
-webkit-border-radius: 2px; 
border-radius: 2px; 
}
input.example { color: #999; font-weight: normal;}
textarea {resize: none;} 

.combobox{
    
background: #efefef; /* Old browsers */
background: -moz-linear-gradient(top,  #efefef 0%, #e8e8e8 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#efefef), color-stop(100%,#e8e8e8)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #efefef 0%,#e8e8e8 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #efefef 0%,#e8e8e8 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #efefef 0%,#e8e8e8 100%); /* IE10+ */
background: linear-gradient(top,  #efefef 0%,#e8e8e8 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#efefef', endColorstr='#e8e8e8',GradientType=0 ); /* IE6-9 */
background: none repeat scroll 0 0 #EEEEEE;
border-top: 1px solid #DDDDDD;
border-right: 1px solid #BBBBBB;
border-bottom: 1px solid #BBBBBB;
border-left: 1px solid #DDDDDD;
-moz-border-radius: 4px !important;
-webkit-border-radius: 4px !important;
border-radius: 4px !important;
-moz-box-shadow: 1px 0 2px #FFFFFF inset;
-webkit-box-shadow: 1px 0 2px #FFFFFF inset;
box-shadow: 1px 0 2px #FFFFFF inset;

display: inline-block;
}

.combobox input:focus{
border: 0 !important; 
-moz-box-shadow: 0 0 0 !important; 
-webkit-box-shadow: 0 0 0 !important; 
box-shadow: 0 0 0 !important; 
outline-width: 0;
}
input.dropdown{
box-shadow: none;
border: 0;
color: #555;
background: transparent;
padding-left: 10px; 
padding-top: 2px; 
padding-bottom: 2px;
padding-right: 0 !important;
}
.ui-button.dropdown{
-moz-border-radius:0;
-webkit-border-radius: 0;
border-radius:0;
box-shadow: none !important;
-moz-box-shadow: 0 0 0 !important;
-webkit-box-shadow: 0 0 0 !important;
box-shadow: 0 0 0 !important;
border: 0 !important;
background: transparent !important;
border-left: 1px solid #E5E5E5 !important;
top: 1px;
width: 30px;
height: 22px;
position: relative;

}
.ui-button.dropdown .ui-button-text{width: 40px; padding: 0;}
.ui-widget input, .ui-widget select, .ui-widget textarea, .ui-widget button {font-family: inherit;}

.ui-autocomplete, .pac-container {
border-radius: 0 0 3px 3px; 
border: 1px solid #ccc; 
border-top: 0; 
box-shadow: 0 2px 5px #aaa;
max-height: 125px;
overflow: auto;
min-width: 162px !important;
margin-top: 2px;
z-index: 10;}
.combobox{z-index: 5;}
.ui-autocomplete .ui-menu-item{
overflow: hidden;
white-space: nowrap;
}

.pac-container{margin-left: 1px; padding: 2px 0!important;}
.ui-menu .ui-menu-item a, .pac-item{
display: block;
font-size: 12px;
line-height: 20px;
padding: 0.2em 0.4em 0.2em 9px;
text-decoration: none;
text-overflow: ellipsis;
overflow: hidden;
}
.pac-item{margin: 0 2px !important;}
.ui-menu .ui-menu-item a.ui-state-hover, .pac-selected{
border: 0 !important; 
border-radius:0 !important;  
background: #5E9BAE !important; 
color: #fff;
margin: 0; 
}

.toggle-switch{
background: #5E9BAE;
padding:  3px 28px 3px 7px;
color: #fff !important; 
font-weight: bold !important;
height: 16px;
line-height: 16px;
display: inline-block;
position: relative;
top: 0; 
right: 0; 
vertical-align: middle;
margin: 0;
border: 1px solid #666;
background: #5e9bae; /* Old browsers */
background: -moz-linear-gradient(top,  #5e9bae 0%, #3791ad 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#5e9bae), color-stop(100%,#3791ad)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #5e9bae 0%,#3791ad 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #5e9bae 0%,#3791ad 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #5e9bae 0%,#3791ad 100%); /* IE10+ */
background: linear-gradient(top,  #5e9bae 0%,#3791ad 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#5e9bae', endColorstr='#3791ad',GradientType=0 ); /* IE6-9 */
-moz-box-shadow: inset -2px -2x 5px #000;
-webkit-box-shadow: inset -2px -2x 5px #000;
box-shadow: inset -2px -2x 5px #000;
text-shadow: -1px -1px 0 #145D73;
min-width: 30px; 
}
.toggle-switch.off{
padding: 3px 7px 3px 28px;  
border: 1px solid #999;
background: #dddddd; /* Old browsers */
background: -moz-linear-gradient(top,  #dddddd 0%, #cccccc 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#dddddd), color-stop(100%,#cccccc)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #dddddd 0%,#cccccc 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #dddddd 0%,#cccccc 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #dddddd 0%,#cccccc 100%); /* IE10+ */
background: linear-gradient(top,  #dddddd 0%,#cccccc 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#dddddd', endColorstr='#cccccc',GradientType=0 ); /* IE6-9 */
color: #777 !important;
text-shadow: 1px 1px 0 #fff;
}
.toggle-switch:after{
display: block; 
position: absolute;
height: 14px; 
width: 20px; 
top: 0;
left: auto;
right: 0; 
background: #eee;
content: "";
padding: 3px 0;
border: 1px solid;
border-color: #eee #ccc #ccc #eee;
box-shadow: 0 0 5px #145D73;
}
.toggle-switch.off:after{
right: auto; 
left: 0;
box-shadow: 0 0 5px #999;
}
